            emailsubject = "MTM Overdue Communications Report for " + DateTime.Now.ToShortDateString();
            
            string filename = Dts.Variables["User::OutputFileName"].Value.ToString();
            int dayslookback=Convert.ToInt32(Dts.Variables["User::DaysPastLookback"].Value.ToString());
            int dayssinceapproval=Convert.ToInt32(Dts.Variables["User::DaysSinceApproval"].Value.ToString());

            emailbody="The following report shows 'approved' communications that have been approved in the last " + dayssinceapproval + " day(s) or " + DateTime.Now.AddDays(-dayssinceapproval).ToShortDateString() + " that we do not have a Mail Date on from our vendor. This excludes approvals within the last " + dayslookback.ToString() +  " day(s) or " + DateTime.Now.AddDays(-dayslookback).ToShortDateString() + " as these may not have been mailed yet";
            emailbody=emailbody+System.Environment.NewLine;
            emailbody="The report is located here: " + filename.Replace(" ",@"%20");
            emailbody=emailbody+System.Environment.NewLine;
            emailbody=emailbody+System.Environment.NewLine;
            emailbody=emailbody+System.Environment.NewLine;
            emailbody=emailbody+System.Environment.NewLine;

            Dts.Variables["User::EmailBody"].Value = emailbody;
            Dts.Variables["User::EmailSubject"].Value = emailsubject;

            Dts.TaskResult = (int)ScriptResults.Success;
        }
    }
}]]></ProjectItem><ProjectItem Name="\properties\resources.designer.cs">
