<![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using System.Xml;
using System.IO;
using System.Data.OleDb;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Text.RegularExpressions;
using System.Text;

namespace ST_afee2e4761e044dc9996604edf39744c.csproj
{
    [System.AddIn.AddIn("ScriptMain", Version = "1.0", Publisher = "", Description = "")]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            // TODO: Add your code here

            OleDbDataAdapter oleDA = new OleDbDataAdapter();
            DataTable dt = new DataTable();
            byte[] pdf;
            FileStream target=null;
            String filname;


            oleDA.Fill(dt, Dts.Variables["pdfVar"].Value);
            int fileCol = -1;
            foreach (DataColumn col in dt.Columns){
                fileCol = col.ColumnName.ToUpper() == "FILENAME" ? col.Ordinal : fileCol;
            }
            
            foreach (DataColumn col in dt.Columns)
            {
                if (col.DataType.IsArray) {
                foreach (DataRow row in dt.Rows)
                {
                    pdf = (byte[])row[col.Ordinal];
                    int pgcount = (bool)Dts.Variables["usePageCount"].Value ? GetNoOfPagesPDF(pdf) : 0;
                    filname = Dts.Variables["pdfTargetDirectory"].Value.ToString();

                    //MessageBox.Show(filname);
                    try
                    {
                        DirectoryInfo d = (bool)Dts.Variables["usePageCount"].Value ? Directory.CreateDirectory(filname + "\\" + (String)pgcount.ToString()) : null;
                    }
                    catch (IOException ex) { }
                    finally
                    {
                        filname = Path.Combine(Path.Combine(filname, (bool)Dts.Variables["usePageCount"].Value ? (String)pgcount.ToString():"" ), (String)row[fileCol].ToString());
                        try
                        {
                            target = File.Open(filname, System.IO.FileMode.Truncate);
                        }
                        catch (FileNotFoundException ex)
                        {
                            target = File.Open(filname, System.IO.FileMode.OpenOrCreate);
                        }
                        finally
                        {
                            target.Write(pdf, 0, pdf.GetLength(0));
                            target.Close();
                            target = null;
                        }
                    }
                }
                }
            }
            Dts.TaskResult = (int)ScriptResults.Success;
        }

public static int GetNoOfPagesPDF(byte[] pdf)
        {
            int result = 0;

            
            ASCIIEncoding enc = new ASCIIEncoding();
            string pdfText = enc.GetString(pdf);

    
            Regex regx = new Regex(@"/Type\s*/Page[^s]");
            MatchCollection matches = regx.Matches(pdfText);
            result = matches.Count;
            return result;
 
        }
    }
}]]></ProjectItem><ProjectItem Name="\properties\resources.designer.cs">
